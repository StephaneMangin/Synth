<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<fx:root type="javafx.scene.layout.Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane prefHeight="60" prefWidth="60" xmlns="http://javafx.com/javafx/8.0.40"
                    xmlns:fx="http://javafx.com/fxml/1">
            <Label fx:id="title" layoutX="0.0" layoutY="0.0" prefHeight="18.0" prefWidth="60.0" text=""
                   textAlignment="CENTER">
                <font>
                    <Font name="System" size="8.0"/>
                </font>
            </Label>
            <Button fx:id="rotatorDial" layoutX="13.0" layoutY="20.0" mnemonicParsing="false" prefHeight="36.0"
                    prefWidth="36.0" styleClass="rotation-dial" text=""/>
            <Button fx:id="rotatorHandle" disable="true" layoutX="13.0" layoutY="20.0" mnemonicParsing="false"
                    opacity="1.0" prefHeight="36.0" prefWidth="36.0" styleClass="rotation-handle" text=""/>
            <Label fx:id="minValue" layoutY="50.0" text="" prefWidth="20" nodeOrientation="RIGHT_TO_LEFT" layoutX="0.0">
                <font>
                    <Font name="System" size="6.0"/>
                </font>
            </Label>
            <Label fx:id="maxValue" layoutX="44.0" layoutY="50.0" text="" prefWidth="20">
                <font>
                    <Font name="System" size="6.0"/>
                </font>
            </Label>
        </AnchorPane>
    </children>
</fx:root>
