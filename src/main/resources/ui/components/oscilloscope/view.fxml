<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="anchorPane" prefHeight="350.0" prefWidth="610.0" styleClass="component" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.istic.synthlab.components.oscilloscope.Controller">

    <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="5.0" layoutY="5.0" pickOnBounds="true"
               preserveRatio="true" smooth="true" onMouseClicked="#close">
        <Image url="@../../images/closeIcon.png"/>
    </ImageView>

    <Label layoutX="35.0" layoutY="4.0" text="Oscilloscope">
        <font>
            <Font name="System" size="20.0"/>
        </font>
    </Label>

    <Pane fx:id="pane" layoutX="5.0" layoutY="35.0" />

    <Label layoutX="11.0" layoutY="290.0" text="Input" />
    <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="15.0" layoutY="310.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#connectInput">
        <Image url="@../../images/blueSocket.png"/>
    </ImageView>

    <Label layoutX="560.0" layoutY="290.0" text="Output" />
    <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="570.0" layoutY="310.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#connectOutput">
        <Image url="@../../images/redSocket.png"/>
    </ImageView>
</AnchorPane>
