<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="oscilloscopePane" maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: #DDD;" styleClass="component" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.istic.synthlab.components.oscilloscope.Controller">
    <Group autoSizeChildren="false">
    <Pane>
   <GridPane fx:id="pane" layoutX="5.0" layoutY="35.0" prefHeight="123.0" prefWidth="290.0">
     <columnConstraints>
       <ColumnConstraints hgrow="NEVER" minWidth="10.0" maxWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" maxHeight="30.0" vgrow="NEVER" />
     </rowConstraints>
   </GridPane>
    </Pane>
    </Group>

    <Label layoutX="6.0" layoutY="2.0" text="Oscilloscope">
        <font>
         <Font name="System" size="20.0" />
        </font>
    </Label>

    <Label layoutX="12.0" layoutY="158.0" text="IN" textAlignment="CENTER">
        <font>
         <Font size="10.0" />
        </font>
    </Label>
    <Circle fx:id="input" layoutX="18.0" layoutY="182.0" onMouseClicked="#connectIn" radius="10.0" stroke="#2600ff" strokeType="INSIDE" strokeWidth="5.0" />

    <Label layoutX="272.0" layoutY="158.0" text="OUT">
        <font>
         <Font size="10.0" />
        </font>
    </Label>
    <Circle fx:id="output" layoutX="283.0" layoutY="182.0" onMouseClicked="#connectOut" radius="10.0" stroke="RED" strokeType="INSIDE" strokeWidth="5.0" />
</AnchorPane>